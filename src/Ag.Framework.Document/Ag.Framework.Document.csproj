    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

	<PropertyGroup>
		<PackageId>Ag.Framework.Document</PackageId>
		<Version>0.1.0</Version>
		<Authors>AGS</Authors>
		<Company>Assured Guaranty</Company>
		<Description>Base Framework for Document Management Systems</Description>
		<Copyright>Copyright (c) Assured Guaranty 2023</Copyright>
	</PropertyGroup>

	<ItemGroup>
		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
			<_Parameter1>$(AssemblyName).Test</_Parameter1>
		</AssemblyAttribute>
	</ItemGroup>

	<ItemGroup>
    <PackageReference Include="Ag.CleanCode" Version="2.1.0" PrivateAssets="All" />
	</ItemGroup>
</Project>
